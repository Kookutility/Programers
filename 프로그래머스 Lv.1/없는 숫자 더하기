def solution(numbers):
    # 결과를 저장할 변수를 초기화합니다.
    answer = 0

    # 0부터 9까지의 숫자를 반복하면서
    for i in range(10):
        # 현재 숫자 i가 numbers 배열에 없는 경우에만 실행합니다.
        if i not in numbers:
            # 현재 숫자 i를 결과에 더합니다.
            answer += i
        
    # 모든 반복이 끝나면 결과를 반환합니다.
    return answer
